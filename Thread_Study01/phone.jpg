<script>
function getUrlParam(name)
{
var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)");
var r = window.location.search.substr(1).match(reg);
if (r!=null) return unescape(r[2]); return null;
}
var pic = getUrlParam("picurl");
pic = pic.replace('http://img.ivsky.com','');
var html = getUrlParam("pichtml");
var ie = getUrlParam("ie");
if(ie == 1){
	document.writeln("<div style=\"background-color:#eee;\">你的浏览器可能太老了，不支持直接下载图片，所以跳转到这个页面，推荐使用Chrome、Safari、360浏览器极速模式</div>");
}

document.writeln("<div style=\"background-color:#eee;\"><img id=\"img\" src='//img.ivsky.com"+pic+"' width='100%'></div>");
// document.writeln("<div><a href='"+html+"' >点这里返回刚才的页面</a><br></div>");

</script>
<div id="dinfo" style="background-color:#ccc;">莫急，图片正在加载中...</div>
<script type="text/javascript">
document.getElementById('img').onload=function(){  document.getElementById('dinfo').innerHTML ='嗯，大概加载完成了...';  };
